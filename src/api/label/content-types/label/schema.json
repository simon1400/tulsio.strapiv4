{
  "kind": "collectionType",
  "collectionName": "labels",
  "info": {
    "singularName": "label",
    "pluralName": "labels",
    "displayName": "Blog labels",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "inversedBy": "labels"
    },
    "meta": {
      "type": "component",
      "repeatable": false,
      "component": "seo.meta"
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "blue",
        "green",
        "pink"
      ],
      "default": "blue"
    }
  }
}
